    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Websocket Server",
            "type": "python",
            "request": "launch",
            "module": "server",
            "env": {
                "WEBSOCKET_APP": "server/server.py",
                "WEBSOCKET_ENV": "development",
                "WEBSOCKET_DEBUG": "0"
            },
        },
        {
            "name": "Python: client/client.py",
            "type": "python",
            "request": "launch",
            "module": "client",
            "env": {
                "WEBSOCKET_APP": "client/client.py",
                "WEBSOCKET_ENV": "development",
                "WEBSOCKET_DEBUG": "0"
            },
        }
    ]
}